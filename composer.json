{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "asgrim/ofxparser": "^1.1",
        "davidhsianturi/blade-bootstrap-icons": "^1.5",
        "doctrine/dbal": "*",
        "endeken/ofx-php-parser": "^0.1.1",
        "guzzlehttp/guzzle": "^7.8",
        "inertiajs/inertia-laravel": "*",
        "jenssegers/agent": "^2.6",
        "knplabs/knp-snappy": "^1.5",
        "laravel-lang/common": "*",
        "laravel-lang/lang": "*",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.7",
        "maatwebsite/excel": "^1.1",
        "mckenziearts/laravel-notify": "^2.5",
        "nfephp-org/nfephp": "^4.0",
        "nfephp-org/sped-nfe": "^5.2",
        "owen-it/laravel-auditing": "^13.6",
        "php-flasher/flasher-laravel": "^2.1",
        "php-flasher/flasher-notyf-laravel": "^2.1",
        "php-flasher/flasher-toastr-laravel": "^2.1",
        "phpoffice/phpspreadsheet": "^5.3",
        "setasign/fpdi": "^2.6",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "sopamo/laravel-filepond": "^1.4",
        "spatie/browsershot": "^5.0",
        "spatie/laravel-permission": "^6.16",
        "spatie/pdf-to-image": "^1.2",
        "stancl/tenancy": "*",
        "symfony/css-selector": "^7.4",
        "symfony/dom-crawler": "^8.0",
        "symfony/http-client": "^8.0",
        "webpatser/laravel-uuid": "^4.0",
        "yajra/laravel-datatables-oracle": "^11.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "lucascudo/laravel-pt-br-localization": "*",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/FileHelper.php",
            "app/Helpers/BrowsershotHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
